{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.1.0",
  "description": "A Vite + React + TypeScript Starter",
  "main": "src/main.tsx",
  "type": "module",
  "scripts": {
    "clean": "rimraf dist",
    "dev": "vite",
    "build": "tsc && vite build",
    "eslint:check": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "eslint:fix": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --fix",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "isomorphic-fetch": "3.0.0",
    "jest-environment-jsdom": "29.7.0",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "6.4.2",
    "@testing-library/react": "15.0.2",
    "@types/jest": "29.5.12",
    "@types/react": "18.2.79",
    "@types/react-dom": "18.2.25",
    "@typescript-eslint/eslint-plugin": "7.7.0",
    "@typescript-eslint/parser": "7.7.0",
    "@vitejs/plugin-react": "4.2.1",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "eslint-plugin-react": "7.34.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-react-refresh": "0.4.6",
    "identity-obj-proxy": "3.0.0",
    "jest": "29.7.0",
    "prettier": "3.2.5",
    "rimraf": "5.0.5",
    "ts-jest": "29.1.2",
    "ts-node": "10.9.2",
    "typescript": "5.4.5",
    "vite": "5.2.10"
  }
}
