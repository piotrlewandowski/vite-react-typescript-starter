{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.1.0",
  "description": "A Vite + React + TypeScript Starter",
  "main": "src/main.tsx",
  "type": "module",
  "scripts": {
    "clean": "rimraf dist",
    "dev": "vite",
    "build": "tsc && vite build",
    "eslint:check": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "eslint:fix": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --fix",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "isomorphic-fetch": "3.0.0",
    "jest-environment-jsdom": "29.7.0",
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "6.4.6",
    "@testing-library/react": "16.0.0",
    "@types/jest": "29.5.12",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "@typescript-eslint/eslint-plugin": "7.15.0",
    "@typescript-eslint/parser": "7.15.0",
    "@vitejs/plugin-react": "4.3.1",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jsx-a11y": "6.9.0",
    "eslint-plugin-react": "7.34.3",
    "eslint-plugin-react-hooks": "4.6.2",
    "eslint-plugin-react-refresh": "0.4.7",
    "identity-obj-proxy": "3.0.0",
    "jest": "29.7.0",
    "prettier": "3.3.2",
    "rimraf": "5.0.7",
    "ts-jest": "29.1.5",
    "ts-node": "10.9.2",
    "typescript": "5.4.5",
    "vite": "5.3.3",
    "vite-plugin-eslint": "1.8.1"
  }
}
